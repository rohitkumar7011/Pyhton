pattern for N = 4
   1
  232
 34543
4567654

n=int(input())
i=1
while i<=n:
    sp=1
    while sp<=n-i:
       print(' ',end='')
       sp=sp+1
    j=1
    while j<=i:
        print(j+i-1,end='')
        j=j+1
    k=1
    p=(2*i)-2
    while k<=i-1:
        print(p,end='')
        k=k+1
        p=p-1
        
    print()
    i=i+1

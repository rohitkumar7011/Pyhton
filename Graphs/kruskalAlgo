class Edge:
    def __init__(self, src, dest ,wt):
        self.src = src
        self.dest = dest
        self.wt  = wt





li = [int(ele) for ele in input().split()]
n = li[0]
e = li[1]
edges = []
def kruskal(edges , nvertices):
    parents = [i for i in range(nvertices)]
    edges = sorted(edegs , key = lambda edge:edge.wt)
    count = 0 

for i in range(e):
    current_input = [int(ele) for ele in input().split()]
    src = current_input[0]
    dest = current_input[1]
    wt = current_input[2]
    edge = Edge(src, dest , wt)
    edges.append(edge)
out = kruskal(edges , n)
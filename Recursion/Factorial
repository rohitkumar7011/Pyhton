n = int(input())
def fact(n):
    if n == 0:
        return 1
    return n*fact(n-1)
out = fact(n)
print(out)

###################################################################################3
# This fact function just print the factorial of a number and returns nothing..
def fact(n,ans=1):
    if n == 0:
        print(ans)
        return
    
    ans = ans * n
    fact(n-1,ans)

out = fact(5)

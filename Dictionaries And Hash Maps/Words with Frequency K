s = "this is a word string having many many word"
k=2

output = many, word


def frequency(s,k):
    word = s.split()
    d={}
    for w in word:
        d[w] = d.get(w,0)+1
    for w in d:
        if d[w]==k:
            print(w)

s = "this is a word string having many many word"
k=2
frequency(s,k)

 N = 5

1        1
12      21
123    321
1234  4321
1234554321

n=int(input())
i=1
while i<=n:
    j=1
    while j<=i:
       print(j,end='')
       j=j+1
    sp=1
    while sp<=(2*n-i-j):
        print(' ',end='')
        sp=sp+1
    k=1
    while k<=i:
        print(i-k+1,end='')
        k=k+1
    print()
    i=i+1
